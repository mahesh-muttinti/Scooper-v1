{
  "name": "Scooper",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "rm -rf node_modules",
    "mobile:android": "yarn android && yarn start",
    "mobile:native:clean": "npx react-native clean",
    "mobile:android:clean": "cd android && ./gradlew clean && cd ../",
    "mobile:patch": "bundle exec fastlane bump",
    "mobile:build:production:aab": "yarn mobile:android:clean && cd android && ./gradlew bundleRelease && cd ../ && ./copy.sh",
    "mobile:build:staging:apk": "yarn mobile:android:clean && cd android && ./gradlew assembleRelease --no-daemon&& cd ../ && ./copy_apk.sh"
  },
